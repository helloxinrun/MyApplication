<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleView">
        <attr name="arrowWidth" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
        <attr name="arrowPosition" format="dimension" />
        <attr name="bubbleRadius" format="dimension" />
        <attr name="bubbleLeftTopRadiu" format="dimension" />
        <attr name="bubbleRightTopRadiu" format="dimension" />
        <attr name="bubbleLeftBottomRadiu" format="dimension" />
        <attr name="bubbleRightBottomRadiu" format="dimension" />
        <attr name="bubbleColor" format="color" />
        <attr name="arrowLocation" format="enum">
            <enum name="left" value="0x00" />
            <enum name="right" value="0x01" />
            <enum name="top" value="0x02" />
            <enum name="bottom" value="0x03" />
        </attr>
        <attr name="arrowRelative" format="enum">
            <enum name="begin" value="0x00"/>
            <enum name="center" value="0x01"/>
            <enum name="end" value="0x02"/>
        </attr>
        <attr name="bubbleBgType" format="enum">
            <enum name="color" value="0x00"/>
            <enum name="bitmap" value="0x01"/>
        </attr>
    </declare-styleable>
</resources>